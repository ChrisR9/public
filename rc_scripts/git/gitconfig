[user]
	name = Example User Name
	email = user@example.com

[alias]
    br      = branch -v
    co      = checkout
    graph   = log --oneline --decorate --graph
    hist    = log --graph --pretty='[%h]%d %cd %s'
    pudr    = push --dry-run
    pu      = push
    stat    = status
    re      = remote -v
    bare    = init --bare --shared=true

[color]
	ui = true

