#!/bin/bash
# git aliases
GIT_PATH="/Users/brian/src/gtk-perl"
alias gtkrefchk="/bin/sh ~/bin/git-tool.sh --path=${GIT_PATH} refchk"
# these next two are the same
alias gtkstat="/bin/sh ~/bin/git-tool.sh --path=${GIT_PATH} repostat"
alias gtkrepostat="/bin/sh ~/bin/git-tool.sh --path=${GIT_PATH} repostat"
alias gtkinchk="/bin/sh ~/bin/git-tool.sh --path=${GIT_PATH} inchk"
alias gtkoutchk="/bin/sh ~/bin/git-tool.sh --path=${GIT_PATH} outchk"
alias gtkpullall="/bin/sh ~/bin/git-tool.sh --path=${GIT_PATH} pullorigin"
alias gtkpullgh="/bin/sh ~/bin/git-tool.sh --path=${GIT_PATH} pullgithub"
unset GIT_PATH
# vi: set filetype=sh shiftwidth=4 tabstop=4
# fin!
